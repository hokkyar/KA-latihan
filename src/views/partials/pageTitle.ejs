<div class="pagetitle">
  <h1>
    <%= title %>
  </h1>
  <nav>
    <ol class="breadcrumb">
      <% if (sub==='' || sub==='Kelola' || sub==='QR' ) { %>
        <li class="breadcrumb-item"><a href="/admin/dashboard">Home</a></li>
        <% }else{ %>
          <li class="breadcrumb-item"><a href="/admin/dashboard">Other</a></li>
          <% } %>
            <% if (sub==='Kelola' ) { %>
              <li class="breadcrumb-item">Kelola</li>
              <% } %>
                <li class="breadcrumb-item active">
                  <a href="/admin/<%= (page === 'show-user') ? 'user-handler' : page %>">
                    <%= title %>
                  </a>
                </li>
                <% if (detail ) { %>
                  <li class="breadcrumb-item">
                    <%= detail.id %>
                  </li>
                  <% } %>
    </ol>
  </nav>
</div>